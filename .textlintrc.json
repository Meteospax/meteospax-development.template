{
  "plugins": {},
  "filters": {},
  "rules": {
    "date-weekday-mismatch": true,
    "editorconfig": true,
    "footnote-order": true,
    "no-curly-quotes": true,
    "no-todo": false,
    "period-in-header": true,
    "spelling": {
